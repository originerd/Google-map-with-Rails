<%= form_for(@google) do |f| %>
  <% if @google.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@google.errors.count, "error") %> prohibited this google from being saved:</h2>

      <ul>
      <% @google.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :latlng %><br>
    <%= f.text_field :latlng, class: "form-control" %>
  </div>
  <div class="field">
    <%= f.label :address %><br>
    <%= f.text_field :address, class: "form-control" %>
  </div>
  <div class="field">
    <%= f.label :content %><br>
    <%= f.text_area :content, rows: 38, class: "form-control" %>
  </div>
  <br>
  <div class="actions">
    <%= f.submit 'Submit', class: "btn btn-lg btn-block btn-primary" %>
  </div>
<% end %>
