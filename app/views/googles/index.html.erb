<p id="notice"><%= notice %></p>

<h1>Listing Googles</h1>

<script>
var map;
function initialize() {
  var mapOptions = {
    zoom: 9,
    center: new google.maps.LatLng(36.593055, 128.173464)
  };
  map = new google.maps.Map(document.getElementById('map-canvas'),
      mapOptions);

  <%= render @googles %>
}

google.maps.event.addDomListener(window, 'load', initialize);

initialize()
</script>

<div id="map-canvas"></div>

<table>
  <thead>
    <tr>
      <th>Latlng</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @googles.each do |google| %>
      <tr>
        <td><%= google.latlng %></td>
        <td><%= google.content %></td>
        <td><%= link_to 'Show', google %></td>
        <td><%= link_to 'Edit', edit_google_path(google) %></td>
        <td><%= link_to 'Destroy', google, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Google', new_google_path %>
